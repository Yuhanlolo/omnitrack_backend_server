  "description": "OmniTrack official backend server.",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build --aot --prod",
    "start": "node dist/backend/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"ng serve --host 0.0.0.0 --disable-host-check --proxy-config proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/backend/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build --aot --prod && tsc -p server && node dist/backend/server/app.js\"",
    "backend-pm2": "npm run predev && pm2 delete omnitrack-backend ; pm2 start backend-pm2.config.js --env=production",
    "backend-forever": "npm run predev && forever start forever.json",
    "dev-frontend": "ng serve --proxy-config proxy.conf.json --open",
    "test": "ng test",
    "testbe": "tsc -p server && node_modules/.bin/mocha dist/backend/server/test",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e",
    "bundle-report": "webpack-bundle-analyzer dist/public/stats.json",
    "version": "cross-var echo \"export const PACKAGE_VERSION=\\\"$npm_package_version\\\"\" > client/app/release_version.ts && git add client/app/release_version.ts"
  },
