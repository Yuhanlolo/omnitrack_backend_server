  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "node dist/server/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"ng serve --host 0.0.0.0 --disable-host-check -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build -aot -prod && tsc -p server && node dist/server/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && node_modules/.bin/mocha dist/server/test",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e"
  },
